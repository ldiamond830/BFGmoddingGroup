//using rocket ammo sprite as place holder
Class IcicleAmmo : Ammo
{
  Default
  {
    Inventory.PickupMessage "Icicle";
    Inventory.Amount 25;
    Inventory.MaxAmount 200;
    Ammo.BackpackAmount 25;
    Ammo.BackpackMaxAmount 400;
    Inventory.Icon "ROCKA0";
    Scale 0.4;
	Tag "Icicle Ammo";
  }

  States
  {
  Spawn:
    ROCK A 1;
    Loop;
  }
}

//using bfg ammo sprite as place holder
Class RatGrenadeAmmo : Ammo{
	Default
	  {
		Inventory.PickupMessage "Rat!";
		Inventory.Amount 2;
		Inventory.MaxAmount 300;
		Ammo.BackpackAmount 20;
		Ammo.BackpackMaxAmount 600;
		Inventory.Icon "RATPA0";
		
		Tag "Rat Grenade Ammo";
	  }

	  States
	  {
	  Spawn:
		RATP A 1;
		Loop;
	  }
}